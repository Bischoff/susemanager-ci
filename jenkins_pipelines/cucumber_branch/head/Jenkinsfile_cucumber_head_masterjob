#!/usr/bin/env groovy

properties([
    buildDiscarder(logRotator(numToKeepStr: '600', daysToKeepStr: '7')),
    disableConcurrentBuilds(),
])

pipeline {
    agent {label 'sumaform-cucumber' }
    triggers { cron('H * * * *') }
    stages {
      stage('Matrix cucumber') {
          steps {  
          script {
            try {
              build job: 'manager-Head-cucumber-sle15', wait: true
             }
            finally {
              build job: 'manager-Head-cucumber-sle11sp4', wait: true
             }
             }
          }
        }
      }
   }
